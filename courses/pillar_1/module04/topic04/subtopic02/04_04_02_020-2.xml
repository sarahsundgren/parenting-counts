					<event type="CLICK" result="SHOW" target="reference01" result_param=""/>
				</eventlist>
			</graphic>
		</container>
		<container id="panel07" behavior="LOCK_SIZE" x="408.65" y="212.9" visible="true" fx="PANEL_SHOW" hidefx="PANEL_HIDE" style="">
			<graphic id="background" behavior="LOCK_XY" x="0" y="0" width="508" height="238" style="PANEL" visible="true" fx="">
				<file play="false" bytes="54" startpos="1">clear.gif</file>
