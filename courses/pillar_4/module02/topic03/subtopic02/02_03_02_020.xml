		<container id="panel02" behavior="" x="496.2" y="223.45" visible="true" fx="PANEL_SHOW" hidefx="PANEL_HIDE" style="">
			<graphic id="background" behavior="LOCK_XY" x="0" y="0" width="538" height="228" style="PANEL" visible="true" fx="">
				<file play="false" bytes="54" startpos="1">clear.gif</file>
