				<file play="false" bytes="54" startpos="1">clear.gif</file>
			</graphic>
			<graphic id="graphic" x="0" y="0" width="" height="" style="DEFAULT" visible="true" fx="">
				<file play="false" bytes="41810" startpos="1">03_01_02_020_03.jpg</file>
				<notes id="description">#185 17 mo. Shea Qin - 67-75 from Exploration folder in Learning &amp; Cognition</notes>
			</graphic>
			<text id="header" x="275" y="10" width="300" height="" autosize="H" style="PANEL_HEADER" visible="true" fx="">
				<content translate="true">Toddlers</content>
			</text>
			<text id="text" x="275" y="35" width="425" height="" autosize="H" style="PANEL" visible="true" fx="">
				<content translate="true">&lt;LI&gt;Develop and learn through testing&lt;/LI&gt;&lt;LI&gt;Grow increasingly independent&lt;/LI&gt;&lt;LI&gt;Intentionally disobey or disregard parents' wishes&lt;/LI&gt;&lt;LI&gt;Learn about conflict&lt;/LI&gt;&lt;LI&gt;Learn what happens when people want different things&lt;/LI&gt;&lt;LI&gt;Observe, watch, and learn before forming and testing hypotheses&lt;/LI&gt;</content>
			</text>
			<graphic id="ref_icon03" x="613" y="112" width="" height="" style="LINK_SWF" visible="true" fx=""/>
		</container>
		<container id="panel04" behavior="LOCK_SIZE" x="390.05" y="252.5" visible="true" fx="PANEL_SHOW" hidefx="PANEL_HIDE" style="">
			<graphic id="background" behavior="LOCK_XY" x="0" y="0" width="724" height="180" style="PANEL_SHAPE_WHITE" visible="true" fx="">
