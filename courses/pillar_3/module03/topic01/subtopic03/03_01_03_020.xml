	<section id="options" align="TOP|LEFT" spacing="15x10" autospace="true" columns="2" expanded="true">
		<container id="option01" behavior="STRICT|LOCK_SIZE" x="25" y="148" visible="true" fx="OPTIONS_SHOW" hidefx="OPTIONS_HIDE" style="">
			<graphic id="button" x="0" y="0" width="355" height="85" style="OPTION" visible="true" advscale="9">
				<file bytes="4024" play="false" startpos="1" duration="20">placeholder_mcoption.swf</file>
			</graphic>
