	<section id="swap_graphics" align="TOP|LEFT" spacing="0" autospace="false" columns="0" exclusive="true"/>
	<section id="swap_text" align="" spacing="0" autospace="false" columns="0" exclusive="false" expanded="true">
		<text id="swap_text01" x="177" y="368" width="120" height="" autosize="H" style="DEFAULT" visible="false" fx="TEXT_SHOW" hidefx="TEXT_HIDE">
			<content translate="true">Children develop a set of skills to self-soothe.</content>
		</text>
		<text id="swap_text02" x="339" y="134" width="131" height="" autosize="H" style="SWAP" visible="false" fx="TEXT_SHOW" hidefx="TEXT_HIDE">
			<content translate="true">Children learn to understand how their feelings lead to their actions.</content>
		</text>
		<text id="swap_text03" x="502" y="353" width="121" height="" autosize="H" style="SWAP" visible="false" fx="TEXT_SHOW" hidefx="TEXT_HIDE">
			<content translate="true">Children are allowed to experience the full range of emotions.</content>
		</text>
		<text id="swap_text04" x="661" y="134" width="103" height="" autosize="H" style="DEFAULT" visible="false" fx="TEXT_SHOW" hidefx="TEXT_HIDE">
			<content translate="true">Children want to work with their parents rather than against them.</content>
		</text>
	</section>
