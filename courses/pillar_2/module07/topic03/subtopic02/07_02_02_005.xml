	<section id="presentation_media" expanded="true">
		<graphic id="initial_graphic" x="20" y="120" width="" height="" style="DEFAULT" visible="true" fx="GRAPHIC_SHOW" hidefx="GRAPHIC_HIDE">
			<file play="false" bytes="60951" startpos="1">03_04_02_010_01.jpg</file>
