				<notes id="description">Toddler in full tantrum, istock #: 1790001 or similar</notes>
			</graphic>
			<graphic id="misc_graphic05" x="350" y="80" width="289" height="231" style="SHADING" visible="true" fx="GRAPHIC_SHOW" hidefx="GRAPHIC_HIDE">
