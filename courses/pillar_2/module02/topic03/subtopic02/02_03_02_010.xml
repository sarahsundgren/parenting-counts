		<container id="panel02" behavior="LOCK_SIZE" x="472.6" y="244.25" visible="true" fx="PANEL_SHOW" hidefx="PANEL_HIDE" style="">
			<graphic id="background" behavior="LOCK_XY" x="0" y="0" width="558" height="258" style="PANEL" visible="true" fx="">
				<file play="false" bytes="54" startpos="1">clear.gif</file>
