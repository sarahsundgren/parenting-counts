		<video id="video" behavior="" x="25" y="60" width="340" height="180" visible="true" fx="VIDEO_SHOW" hidefx="VIDEO_HIDE" style="DEFAULT">
			<file play="false" autorewind="true" buffer="15" preload="10" volume="100" bytes="21705401" start_frame="0">medina.mov</file>
			<transcript/>
